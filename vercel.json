{
  "version": 2,
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/css/(.*)", "dest": "/public/css/$1" },
    { "src": "/js/(.*)", "dest": "/public/js/$1" },
    { "src": "/dashboard.html", "dest": "/public/dashboard.html" },
    { "src": "/editor.html", "dest": "/public/editor.html" },
    { "src": "/preview.html", "dest": "/public/preview.html" },
    { "src": "/index.html", "dest": "/public/index.html" },
    { "src": "/", "dest": "/public/index.html" }
  ]
}
